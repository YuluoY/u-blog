    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.4",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.40",
    "postcss-preset-env": "^9.6.0",
    "sass": "^1.68.0",
    "unplugin-auto-import": "^0.17.5",
