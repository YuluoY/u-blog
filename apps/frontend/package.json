{
  "name": "@u-blog/frontend",
  "version": "0.0.1",
  "description": "vue3的ts项目模板",
  "main": "index.js",
  "scripts": {
    "dev": "vite --force",
    "start": "npm run dev",
    "build": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "format": "prettier \"src/**/*.{vue,js,ts,jsx,tsx}\" --write .",
    "prepare": "husky",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint \"src/**/*.{js,jsx,vue,ts,tsx}\" --fix --ignore-path .gitignore .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Yuluo",
  "license": "MIT",
  "peerDependencies": {
    "@u-blog/composables": "workspace:*",
    "@u-blog/helper": "workspace:*",
    "@u-blog/model": "workspace:*",
    "@u-blog/ui": "workspace:*",
    "@u-blog/utils": "workspace:*"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "axios": "^1.7.2",
    "lodash-es": "^4.17.21",
    "md-editor-v3": "^6.0.1",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-i18n": "^9.13.1",
    "vue-router": "4"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": []
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@faker-js/faker": "^8.4.1",
